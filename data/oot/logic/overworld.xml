<?xml version="1.0" encoding="UTF-8"?>
<overworld>
  <scene id="55" name="KF_Main" desc="Kokiri Forest">
    <link to="KF_Mido"/>
    <link to="KF_LinkHouse"/>
    <link to="DT_Main"/>
    <link to="LW_Bridge"/>
    <link to="LW_Main"/>
    <chest id="00" content="SWORD"/>
  </scene>
  <scene id="28" name="KF_Mido" desc="Mido's House">
    <chest id="00" content="RUPEE_BLUE"/>
    <chest id="01" content="RUPEE_BLUE"/>
    <chest id="02" content="RUPEE_GREEN"/>
    <chest id="03" content="RECOVERY_HEART"/>
  </scene>
  <scene id="34" name="KF_LinkHouse" desc="Link's House"/>
  <scene id="5b">
    <room name="LW_Bridge" desc="Lost Woods Bridge">
      <special id="c1" content="OCARINA" desc="Saria"/>
    </room>
    <room name="LW_Main">
      <link to="SacredMeadow"/>
    </room>
  </scene>
  <scene id="56" name="SacredMeadow">
    <special id="58" desc="Saria's Song" content="SONG_SARIA" cond="reach(Hyrule_CastleCourtyard)"/>
  </scene>
  <scene id="51" name="Hyrule_Field" desc="Hyrule Field">
    <link to="LW_Bridge"/>
    <link to="Hyrule_Market"/>
    <link to="Kakariko"/>
    <link to="ZoraRiver_Entrance"/>
  </scene>
  <scene id="20" name="Hyrule_Market">
    <link to="Hyrule_Castle"/>
  </scene>
  <scene id="5f" name="Hyrule_Castle" desc="Hyrule Castle">
    <npc id="00" content="CHICKEN" desc="Malon"/>
    <link to="Hyrule_CastleCourtyard" cond="has(CHICKEN)"/>
  </scene>
  <scene id="4a" name="Hyrule_CastleCourtyard" desc="Hyrule Castle Courtyard">
    <special id="40" content="ZELDA_LETTER"/>
    <special id="59" content="SONG_ZELDA"/>
  </scene>
  <scene id="52" name="Kakariko">
    <link to="Graveyard"/>
    <link to="DeathMountain" cond="has(ZELDA_LETTER)"/>
    <npc id="01" content="EMPTY_BOTTLE" desc="Anju (Child)"/>
  </scene>
  <scene id="53" name="Graveyard">
    <link to="Graveyard_TombReDead"/>
    <link to="Graveyard_TombFairy"/>
    <link to="Graveyard_TombRoyal" cond="can_play(SONG_ZELDA) and has(SWORD)"/>
  </scene>
  <scene id="3f" name="Graveyard_TombReDead">
    <chest id="00" content="HEART_PIECE" cond="can_play(SONG_SUN)"/>
  </scene>
  <scene id="40" name="Graveyard_TombFairy">
    <chest id="00" content="SHIELD"/>
  </scene>
  <scene id="41" name="Graveyard_TombRoyal">
    <!-- <chest id="00" content="BOMBS_10"/> Requires fire -->
    <special id="5a" content="SONG_SUN"/>
  </scene>
  <scene id="60" name="DeathMountain">
    <link to="GoronVillage"/>
    <link to="DC_Main" cond="has_bombflowers and has(SWORD)"/>
    <link to="GreatFairy_Magic1" cond="can_play(SONG_ZELDA) and has_explosives"/>
    <chest id="01" content="RUPEE_PURPLE" cond="has_explosives"/>
  </scene>
  <scene id="62" name="GoronVillage">
    <npc id="02" content="STRENGTH" cond="has(OCARINA) and has(SONG_ZELDA) and has(SONG_SARIA)"/>
  </scene>
  <scene id="3b" name="GreatFairy_Magic1">
    <collectible id="00" content="MAGIC_UPGRADE"/>
  </scene>
  <scene id="54" desc="Zora River">
    <room name="ZoraRiver_Entrance">
      <link to="ZoraRiver" cond="has_explosives"/>
    </room>
    <room name="ZoraRiver">
      <link to="ZoraFountain" cond="can_play(SONG_ZELDA)" flags="one-way"/>
      <collectible id="04" content="HEART_PIECE" desc="Pillar"/>
      <collectible id="0b" content="HEART_PIECE" desc="Waterfall"/>
    </room>
  </scene>
  <scene id="59" name="ZoraFountain" desc="Zora Fountain">
    <link to="ZoraRiver" flags="one-way"/>
    <chest id="00" content="HEART_PIECE" desc="Waterfall Chest"/>
    <npc id="03" content="SCALE" desc="Diving Game"/>
  </scene>
</overworld>
